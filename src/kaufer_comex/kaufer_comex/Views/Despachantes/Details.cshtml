@model kaufer_comex.Models.Despachante

@{
	ViewData["Title"] = "Details";
}

<br />
<h3>Nome do Despachante: @Html.DisplayFor(model => model.NomeDespachante)</h3>
<hr />
<div>
	<dl class="row">
		<dt class="col-sm-2">
			@Html.DisplayNameFor(model => model.NomeDespachante)
		</dt>
		<dd class="col-sm-10">
			@Html.DisplayFor(model => model.NomeDespachante)
		</dd>
	</dl>
</div>
<div>
	<a asp-action="Edit" asp-route-id="@Model?.Id" class="btn " style=" background-color: #fd7e14; color: #FFF; width: 100px">Editar</a> |
	<a asp-action="Index" class="btn btn-dark" style="width: 100px">Voltar</a>
</div>
